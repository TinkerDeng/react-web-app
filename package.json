    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "cls": "^0.1.5",
    "cross-env": "^5.2.0",
